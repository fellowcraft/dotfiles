<!DOCTYPE patchbay>
<patchbay name="qjack_qsynth_qtractor" version="0.4.2">
 <output-sockets>
  <socket type="jack-audio" name="qsynth 1" exclusive="off" client="qsynth">
   <plug>l_00</plug>
   <plug>r_00</plug>
  </socket>
  <socket type="jack-audio" name="Qtractor 1" exclusive="off" client="Qtractor">
   <plug>Master/out_1</plug>
   <plug>Master/out_2</plug>
  </socket>
  <socket type="jack-audio" name="PulseAudio JACK Sink 1" exclusive="off" client="PulseAudio JACK Sink">
   <plug>front-left</plug>
   <plug>front-right</plug>
  </socket>
  <socket type="alsa-midi" name="Qtractor MIDI" exclusive="off" client="Qtractor">
   <plug>Master</plug>
  </socket>
  <socket type="jack-audio" name="system 1" exclusive="off" client="system">
   <plug>capture_1</plug>
   <plug>capture_2</plug>
  </socket>
 </output-sockets>
 <input-sockets>
  <socket type="jack-audio" name="Qtractor 1" exclusive="off" client="Qtractor">
   <plug>Master/in_1</plug>
   <plug>Master/in_2</plug>
  </socket>
  <socket type="jack-audio" name="system 1" exclusive="off" client="system">
   <plug>playback_1</plug>
   <plug>playback_2</plug>
  </socket>
  <socket type="alsa-midi" name="FLUID Synth \([0-9]+\) 1 MIDI" exclusive="off" client="FLUID Synth \([0-9]+\)">
   <plug>Synth input port \([0-9]+:0\)</plug>
  </socket>
 </input-sockets>
 <slots/>
 <cables>
  <cable type="jack-audio" output="qsynth 1" input="Qtractor 1"/>
  <cable type="jack-audio" output="Qtractor 1" input="system 1"/>
  <cable type="jack-audio" output="PulseAudio JACK Sink 1" input="system 1"/>
  <cable type="alsa-midi" output="Qtractor MIDI" input="FLUID Synth \([0-9]+\) 1 MIDI"/>
 </cables>
</patchbay>

