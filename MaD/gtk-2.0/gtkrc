			image 
			{
			function				= BOX
            file				    = "Scrollbars/stepper.svg"
            border				    = { 0,0,0,0 }
			stretch					= TRUE
			}
			
            ########### ARROWS
            image
            {
            function		    = ARROW
            state               = NORMAL
            recolorable		    = TRUE
            overlay_file		= "Scrollbars/arrow-normal.svg"
            overlay_border		= { 0,0,0,0 }
            overlay_stretch		= FALSE
            }

            image
            {
            function		    = ARROW
            state			    = PRELIGHT
            recolorable		    = TRUE
            overlay_file		= "Scrollbars/arrow-prelight.svg"
            overlay_border		= { 0,0,0,0 }
            overlay_stretch		= FALSE
            }

            image
            {
            function		    = ARROW
            state			    = ACTIVE
            recolorable		    = TRUE
            overlay_file		= "Scrollbars/arrow-active.svg"
            overlay_border		= { 0,0,0,0 }
            overlay_stretch		= FALSE
            }

            image
            {
            function		    = ARROW
            state			    = INSENSITIVE
            recolorable		    = TRUE
            overlay_file		= "Scrollbars/arrow-insensitive.svg"
            overlay_border		= { 0,0,0,0 }
            overlay_stretch		= FALSE
            }
    }
}
widget "*GimpDisplayShell.*" style "pscc-gimp-display-style"


