        state    = ACTIVE
        file     = "Entry/notebook-entry-active.svg"
        border   = { 1,1,1,1 }
        stretch  = TRUE
        }
        image {
        function = SHADOW
        detail   = "entry"
        state    = INSENSITIVE
        shadow   = IN
        file     = "Entry/notebook-entry-insensitive.svg"
        border   = { 1,1,1,1 }
        stretch  = TRUE
        }
    }
}
