style "checkradio"
{
        fg[NORMAL] 		= @fg_color
        fg[PRELIGHT]    = @text-prelight
        fg[ACTIVE]      = @selected_fg_color
        fg[INSENSITIVE] = shade (0.65, @selected_fg_color)


        # check/radio
        base[NORMAL] 		= @bluecolor
        text[NORMAL]        = @selected_fg_color
        
        bg[SELECTED]        = @bluecolor
        text[ACTIVE] 		= @selected_fg_color
        
        text[PRELIGHT]	    = "red"

        # buttons:
		bg[NORMAL]      = "black"
		bg[PRELIGHT]    = mix(0.90, @bg_color, @bluecolor)
		bg[ACTIVE]      = shade(0.95, @bluecolor)
		bg[INSENSITIVE] = "#555656"

	engine "pixmap" 
	{
            image 
            {
            function        	= FLAT_BOX
            recolorable     	= TRUE
            stretch				= TRUE
            file            	= "Assets/null.png"
            border          	= { 0,0,0,0 }
            }
    }
}
