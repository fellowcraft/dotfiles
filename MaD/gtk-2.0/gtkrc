gtk_color_scheme = "base_color:#575F61\nbg_color:#474C4A"
gtk_color_scheme = "fg_color:#E0E0E0\ntext_color:#E6E6E6"
gtk_color_scheme = "selected_bg_color:#141414\nselected_fg_color:#FFFFFF"
gtk-color-scheme = "tooltip_bg_color:#384145\ntooltip_fg_color:#FFFFFF"
gtk_color_scheme = "text-prelight:#FF80EE\nlink_color:#DB9ADB"
gtk_color_scheme = "redcolor:#CC3D97\nbluecolor:#009acd"


gtk-auto-mnemonics		= 1 # Disables ugly lines under menu items
gtk-enable-animations 	= 1  
gtk-tooltip-timeout 	= 1200
gtk-menu-popup-delay 	= 360

