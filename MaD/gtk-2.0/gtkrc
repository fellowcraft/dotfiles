			# Normal:
            base[NORMAL] 		= "#656E70"
            text[NORMAL]    	= @selected_fg_color

			# Selected:
            base[SELECTED]	    = @selected_fg_color
            text[SELECTED]	    = "#000000"

			# Active:
			base[ACTIVE]	    = "#656E70"
			text[ACTIVE]	    = @selected_fg_color

			# insensitive:
			base[INSENSITIVE]   = "#5b615e"
            text[INSENSITIVE] 	= shade(0.65, @selected_fg_color)


            # buttons:
            bg[NORMAL]      = "black"
            bg[PRELIGHT]    = "black"
            bg[ACTIVE]      = "black"
            bg[INSENSITIVE] = "black"

