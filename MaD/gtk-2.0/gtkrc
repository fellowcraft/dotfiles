style "spinbutton"
{
	xthickness = 6
	ythickness = 4

	GtkSpinButton::inner-border = { 4,4,2,2 }

			# Normal:
            base[NORMAL] 		= "#656E70"
            text[NORMAL]    	= @selected_fg_color

			# Selected:
            base[SELECTED]	    = @selected_fg_color
            text[SELECTED]	    = "#000000"

			# Active:
			base[ACTIVE]	    = "#656E70"
			text[ACTIVE]	    = @selected_fg_color

			# insensitive:
			base[INSENSITIVE]   = "#5b615e"
            text[INSENSITIVE] 	= shade(0.65, @selected_fg_color)


            # buttons:
            bg[NORMAL]      = "black"
            bg[PRELIGHT]    = "black"
            bg[ACTIVE]      = "black"
            bg[INSENSITIVE] = "black"

            # Progress-Bar
            bg[SELECTED]	= @bluecolor    # Progress-Bar

            # arrows:
            fg[NORMAL]          = "#FFFFFF"
            fg[PRELIGHT]        = mix(0.40, @selected_fg_color, @bluecolor)
            fg[ACTIVE]          = mix(0.40, @selected_fg_color, @redcolor)
            fg[INSENSITIVE]     = "#999999"


	engine "pixmap"
	{
    # Buttons
		image
		{
		function			= BOX
		detail				= "spinbutton_up"
		recolorable			= TRUE
		file				= "SpinButton/spin_button_up.svg"
		border				= { 1,1,1,1 }
		stretch				= TRUE
		}

		image
		{
		function			= BOX
		detail				= "spinbutton_down"
		recolorable			= TRUE
		file				= "SpinButton/spin_button_down.svg"
		border				= { 1,1,1,1 }
		stretch				= TRUE
		}


    # Arrows
        image
        {
        function		    = ARROW
        state               = NORMAL
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-up.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= UP
        }

        image
        {
        function		    = ARROW
        state			    = PRELIGHT
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-up-prelight.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= UP
        }

        image
        {
        function		    = ARROW
        state			    = ACTIVE
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-up-active.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= UP
        }

        image
        {
        function		    = ARROW
        state			    = INSENSITIVE
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-up-insensitive.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= UP
        }

        image
        {
        function		    = ARROW
        state			    = NORMAL
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-down.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= DOWN
        }

        image
        {
        function		    = ARROW
        state			    = PRELIGHT
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-down-prelight.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= DOWN
        }

        image
        {
        function		    = ARROW
        state			    = ACTIVE
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-down-active.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= DOWN
        }

        image
        {
        function		    = ARROW
        state			    = INSENSITIVE
        recolorable		    = TRUE
        overlay_file		= "SpinButton/arrow-down-insensitive.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
        arrow_direction		= DOWN
        }


	# SPIN ENTRY ##############
		image
		{
		function			= FOCUS
		recolorable			= TRUE
		file				= "Entry/notebook-entry-active.svg"
		border				= { 1,1,1,1 }
		stretch				= TRUE
		}	
